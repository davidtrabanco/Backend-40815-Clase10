{{#if prodFound}}
    <table>
        <tr>
            <th>ID</th>
            <th>Producto</th>
            <th>Precio</th>
            <th>Imagen</th>
        </tr>
        {{#each productsList}}
            <tr>
                <td> {{this.id}} </td>
                <td> {{this.title}} </td>
                <td> {{this.price}} </td>
                <td> <img src="{{this.thumbnail}}" alt=""> </td>
            </tr>
        {{/each}}
    </table>
{{else}}
<h2>No hay productos</h2>   
{{/if}}